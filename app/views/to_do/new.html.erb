<% provide(:title, "Regist ToDo")%>

<%= form_with @todo do |form|%>
    <%= form.label :clear_paln, "完了予定日"%>
    <%= form.date_select :clear_plan, class:"form-control"%>

    <%= f.fields_for :menus, @todo.menus.build do |form_item| %>
        <%= form_item.label :menu_title, "トレーニング名"%>
        <%= form_item.select :menutitle, @menu, class:"form-conrtol"%>
<% end %>