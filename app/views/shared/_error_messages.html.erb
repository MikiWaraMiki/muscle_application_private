
<% if user.errors.any?%>
    <div id="error-messages">
        <div class="error_explanation">
            <div class="alert alert-danger">
                入力内容が誤っています.
            </div>
            <ul>
            <% user.errors.full_messages.each do | msg |%>
                <li><%= msg%></li>
            <% end %>
            </ul>
        </div>
    </div>
<%end%>